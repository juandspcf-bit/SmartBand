<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MyApplication" parent="Theme.Material3.Light.NoActionBar">

        <item name="android:statusBarColor" >@color/white</item>
        <!-- Customize your theme here. -->
        <item name="statusBarBackground">@color/white</item>
        <item name="android:windowTranslucentStatus">false</item>
    </style>

    <style name="DashBoard" parent="Theme.Material3.Light.NoActionBar">
        <item name="android:statusBarColor">@color/white</item>
        <item name="statusBarBackground">@color/white</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="DashBoard2" parent="Theme.Material3.Light.NoActionBar">
        <item name="android:statusBarColor">#c7dacf</item>
        <item name="statusBarBackground">#c7dacf</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

    </style>




</resources>